public class Count_Number_of_Inversions1 {
    public static void main(String[] args) {
        int a[]={1, 20, 6, 4, 5};
        int n=a.length;
        int count=0;
        for (int i = 0; i < n; i++) {
            for (int j = i; j < n; j++) {
                if(a[i]>a[j])count++;
            }
        }
        System.out.println(count);
    }
}
