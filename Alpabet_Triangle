import java.io.*;

class Alpabet_Triangle {
	public static void main (String[] args) {
	    int x=65;
	    int n=5;
	    for(int i=0;i<n;i++){
	        x=65;
	        for(int j=0;j<=i;j++){
	            char c_x=(char)x;
	            System.out.print(" "+c_x);
	            x++;
	        }
	        System.out.println();
        }
	}
}
