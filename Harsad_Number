import java.io.*;

class Harsad_Number {
	public static void main (String[] args) {
	    int n=156;
	    int temp=n,r=0;
	    int sum=0;
	    while(n!=0){
	        r=n%10;
	        sum+=r;
	        n=n/10;
	    }
	    if(sum!=0 && temp%sum==0)System.out.print("Harshad number");
	    else System.out.print(" Not a Harshad number");
	}
}
